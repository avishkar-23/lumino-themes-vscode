      "settings": {
        "foreground": "#56b6c2"
      }
    },
    {
      "name": "Library constant",
      "scope": "support.constant",
      "settings": {
        "fontStyle": "",
        "foreground": "#e5c07b"
      }
    },
    {
      "name": "Library class/type",
      "scope": "support.class",
      "settings": {
        "foreground": "#e5c07b"
      }
    },
    {
      "name": "Library type",
