      "settings": {
        "foreground": "#c678dd"
      }
    },
    {
      "name": "Keyword Operator",
      "scope": "keyword.operator",
      "settings": {
        "foreground": "#61afef"
      }
    },
    {
      "name": "Storage",
      "scope": "storage",
      "settings": {
        "fontStyle": "",
        "foreground": "#c678dd"
      }
    },
    {
      "name": "Storage type",
      "scope": "storage.type.function",
      "settings": {
        "foreground": "#c678dd"
      }
